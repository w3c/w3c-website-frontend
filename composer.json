{
    "name": "w3c/website-frontend",
    "description": "W3C website frontend application",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "chrisguitarguy/request-id-bundle": "^v6.0",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/annotations": "^2.0",
        "ekreative/health-check-bundle": "^1.12",
        "friendsofsymfony/http-cache-bundle": "^3.0",
        "guzzlehttp/promises": "*",
        "nelmio/cors-bundle": "^2.5",
        "nyholm/psr7": "^1.8",
        "phpdocumentor/reflection-docblock": "^5.4",
        "sabre/vobject": "^4.5",
        "strata/symfony-frontend": "^0.9",
        "symfony/asset": "7.1.*",
        "symfony/console": "7.1.*",
        "symfony/dotenv": "7.1.*",
        "symfony/expression-language": "7.1.*",
        "symfony/flex": "^2.4",
        "symfony/form": "7.1.*",
        "symfony/framework-bundle": "7.1.*",
        "symfony/http-client": "7.1.*",
        "symfony/intl": "7.1.*",
        "symfony/mailer": "7.1.*",
        "symfony/mime": "7.1.*",
        "symfony/monolog-bundle": "^v3.10",
        "symfony/notifier": "7.1.*",
        "symfony/process": "7.1.*",
        "symfony/property-access": "7.1.*",
        "symfony/property-info": "7.1.*",
        "symfony/proxy-manager-bridge": "6.4",
        "symfony/security-bundle": "7.1.*",
        "symfony/security-csrf": "7.1.*",
        "symfony/serializer": "7.1.*",
        "symfony/string": "7.1.*",
        "symfony/translation": "7.1.*",
        "symfony/twig-bundle": "7.1.*",
        "symfony/validator": "7.1.*",
        "symfony/web-link": "7.1.*",
        "symfony/yaml": "7.1.*",
        "twig/extra-bundle": "^3.10",
        "twig/twig": "^3.10",
        "w3c/website-templates-bundle": "dev-jg/sf7"
    },
    "require-dev": {
        "overtrue/phplint": "^9.4",
        "phpunit/phpunit": "^11.2",
        "rector/rector": "^1.2",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.10",
        "studio24/deployer-recipes": "dev-update-to-deployer-7",
        "symfony/browser-kit": "^7.1",
        "symfony/css-selector": "^7.1",
        "symfony/debug-bundle": "^7.1",
        "symfony/maker-bundle": "^v1.60",
        "symfony/panther": "^v2.1",
        "symfony/phpunit-bridge": "^7.1",
        "symfony/stopwatch": "^7.1",
        "symfony/var-dumper": "^7.1",
        "symfony/web-profiler-bundle": "^7.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test": [
            "./vendor/bin/phplint",
            "./vendor/bin/phpcs",
            "php ./bin/console lint:container",
            "./vendor/bin/phpunit"
        ]
    },
    "scripts-descriptions": {
        "test": "Run all required tests"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.1.*"
        }
    }
}
